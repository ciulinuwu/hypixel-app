<div data-role="page" id="stats:player">
    <%-include("../ui/header", {name: "Profile", back: "#lookup"}) %>
    <div class="ui-content" data-role="content">
        <block>
            <ul data-role="listview" data-inset="true">
                <% for (let i = 0; i < data.length; i++) { %>
                    <li class="ui-field">
                        <% if (data[i].c) { %>
                        <a href="<%=data[i].c%>">
                        <% } %>
                            <p><%=data[i].a%></p>
                            <p><small class="<%=data[i].b%>"></small></p>
                        <% if (data[i].c) { %>
                        </a>
                        <% } %>
                    </li>
                <% } %>
            </ul>
        </block>
    </div>
</div>