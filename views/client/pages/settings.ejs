<div data-role="page" id="settings">
    <%-include("../ui/header", {name: "Settings", back: "#"}) %>
    <div class="ui-content" data-role="content">
        <div data-role="fieldcontain">
            <label>API Key:</label>
            <input class="Input.Settings.apiKey" type="text" autocorrect="off" />
            <p><small>Note: You can find your API key by logging into the Hypixel server and typing "/api"</small></p>
            <block>
                <p class="Text.Settings.apiKey.SuccessMessage" data-text-color="GREEN">
                    <small></small>
                </p>
                <p class="Text.Settings.apiKey.ErrorMessage" data-text-color="RED">
                    <small></small>
                </p>
            </block>
            <button class="Button.Settings.s_apiKey">Validate</button>
        </div>
    </div>
</div>